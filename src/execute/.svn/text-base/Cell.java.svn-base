package execute;

public abstract class Cell {
	
	private int type;
	
	public Cell(int type) {
		
		this.type = type;
		
	}
	
	/**
	 * Return the type.
	 */
	
	public int getType() {
	
		return type;
	
	}

	/**
	 * Reset the type.
	 */
	
	public void setType(int type) {
	
		this.type = type;
	
	}

	public abstract String toString();

}
